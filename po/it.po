#: src/myoptParser.c:203
#, c-format
msgid "Error: multiple occurrences(%d) of '%s' option.\n"
msgstr "Errore: occorrenze multiple(%d) dell'opzione '%s'.\n"

#: src/myoptParser.c:214
#, c-format
msgid "Error: option %s: wrong number of arguments: must be %d; found %d.\n"
msgstr ""
"Errore: opzione %s: numero di argomenti errato: deve essere %d; trovato %d.\n"

#: src/myoptParser.c:227
#, c-format
msgid ""
"Error: option %s: wrong number of arguments: must be between %d and %d; "
"found %d.\n"
msgstr ""
"Errore: opzione %s: numero di argomenti errato: deve essere tra %d e %d; "
"trovato %d.\n"

#: src/myoptParser.c:236
#, c-format
msgid "Error: option '-%c --%s' is required.\n"
msgstr "Errore: l'opzione '-%c --%s' è obbligatoria.\n"

#: src/myoptParser.c:275
#, c-format
msgid "Error: must be present at least one option of group %d '%s'.\n"
msgstr "Errore: deve essere presente almeno un'opzione del gruppo %d '%s'.\n"

#: src/myoptParser.c:281
#, c-format
msgid ""
"Error: there is more than one option for the mutually exclusive group %d "
"'%s'. It is allowed only one.\n"
msgstr ""
"Errore: è stata specificata più di un'opzione per il gruppo %d '%s'. Ne è "
"permessa soltanto una.\n"

#: src/myoptParser.c:291
#, c-format
msgid "Error: wrong number of positional arguments: must be %d; found %d.\n"
msgstr ""
"Errore: numero errato di argomenti posizionali: deve essere %d; trovato %d.\n"

#: src/myoptParser.c:298
#, c-format
msgid ""
"Error: wrong number of positional arguments: must be between %d and %d; "
"found %d.\n"
msgstr ""
"Errore: numero errato di argomenti posizionali: deve essere tra %d e %d; "
"trovato %d.\n"

#: src/myoptParser.c:304
#, c-format
msgid ""
"Error: wrong number of positional arguments: must be at least %d; found %d.\n"
msgstr ""
"Errore: numero errato di argomenti posizionali: deve essere almeno %d; "
"trovato %d.\n"

#: src/myoptParser.c:418
#, c-format
msgid ""
"Error: option %s: wrong number of arguments: must be almost %d; found %d.\n"
msgstr ""
"Errore: opzione %s: numero errato di argomenti: deve essere almeno %d; "
"trovato %d.\n"

#: src/myoptParser.c:494
msgid "Error: multiple occurrences of '--'.\n"
msgstr "Errore: occorrenze multiple di '--'.\n"

#: src/myoptParser.c:505
msgid "Error: unexpected token.\n"
msgstr "Errore: token inaspettato.\n"

#: src/myoptParser.c:715
#, c-format
msgid "Error: positional argument %d wrong type. Must be %s; found %s: '%s'\n"
msgstr ""
"Errore: tipo dell'argomento posizionale %d errato. Deve essere %s; trovato "
"%s: '%s'\n"

#: src/myoptParser.c:717
#, c-format
msgid ""
"Error: option '%s': argument %d wrong type. Must be %s; found %s: '%s'\n"
msgstr ""
"Errore: opzione '%s': specificato un tipo errato per l'argomento %d. Deve "
"essere %s; trovato %s: '%s'\n"

#: src/myoptLexer.c:86
msgid "Error: wrong option '-'\n"
msgstr "Errore: opzione non valida '-'\n"

#: src/myoptLexer.c:150
#, c-format
msgid ""
"Error: option '%s': there must be no spaces before and after the equal sign\n"
msgstr ""
"Errore: opzione '%s': Non ci devono essere spazi prima e dopo il segno di "
"uguale\n"

#: src/myoptLexer.c:172 src/myoptLexer.c:188
#, c-format
msgid "Error: unrecognized option '%s'\n"
msgstr "Errore: opzione non riconosciuta '%s'\n"

#: src/myoptLexer.c:251
#, c-format
msgid "Error: unrecognized option '-%c'\n"
msgstr "Errore: opzione non riconosciuta '-%c'\n"
